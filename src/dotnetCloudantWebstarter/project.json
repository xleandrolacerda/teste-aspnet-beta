{
    "buildOptions": {
        "copyToOutput": {
            "include": [ "wwwroot", "**.cshtml", "vcap-local.json" ],
            "exclude": [ "bin", "obj" ]
        },
        "debugType": "portable",
        "emitEntryPoint": true,
        "preserveCompilationContext": true
    },

    "dependencies": {
        "Microsoft.AspNetCore.Server.Kestrel": "1.0.2",
        "Microsoft.AspNetCore.Diagnostics": "1.0.1",
        "Microsoft.AspNetCore.Http.Abstractions": "1.0.1",
        "Microsoft.Extensions.Configuration.CommandLine": "1.0.1",
        "Microsoft.Extensions.Configuration.Json": "1.0.1",
        "Microsoft.AspNetCore.Mvc": "1.0.2",
        "Microsoft.AspNetCore.StaticFiles": "1.0.1",
        "Microsoft.Extensions.Logging.Console": "1.0.1",
        "Microsoft.AspNet.WebApi.Client": "5.2.3",
        "Newtonsoft.Json": "9.0.1",
        "Microsoft.NETCore.App": {
            "type": "platform",
            "version": "1.0.3",
            "include": "all"
        },
        "System.Runtime.Serialization.Xml": "4.1.1"
    },

    "frameworks": {
        "netcoreapp1.0": {
            "imports": [
                "dnxcore50",
                "netcore45",
                "portable-net461+netcore45+wp81+wpa81+dnxcore50"
            ]
        }
    },

    "publishOptions": {
        "include": [ "wwwroot", "**.cshtml" ],
        "exclude": [ "bin", "obj" ]
    },

    "scripts": { },

    "version": "1.0.3"
}
